{
    "configurations": [
        {
            "name": "Linux",
            "forcedInclude": [
                "${workspaceFolder}/CMSIS/Core/Include/cmsis_gcc.h",
                "${workspaceFolder}/CMSIS/Core/Include/m-profile/cmsis_gcc_m.h"
            ],
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/newlib/**"
            ],
            "defines": [
                "STM32L476xx"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "compilerArgs": [
                "-mthumb",
                "-mcpu=cortex-m4",
                "-mfpu=fpv4-sp-d16"
            ],
            "cStandard": "c11",
            "cppStandard": "gnu++11",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}